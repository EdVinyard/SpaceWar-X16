INIT:    SCREEN $80    RECT 0,0, 319,239, 0    PRINT "HELLO, SPACEWAR!"        T0 = -2 * 3.14159 / 16    FOR I = 0 TO 15        X1=0: Y1=-4        X2=3: Y2=3        X=X1: Y=Y1: T=I*T0: GOSUB ROTATE        X1=X: Y1=Y        X=Y2: Y=Y2: T=I*T0: GOSUB ROTATE        X2=X: Y2=Y        PRINT X1,Y1,X2,Y2        LINE 310+X1,8*I+Y1+4, 310+X2,8*I+Y2+4, 1        NEXT I    ENDROTATE:    REM ROTATE AN X,Y COORDINATE AROUND THE ORIGIN (0,0)    REM INOUT X X-COORDINATE    REM INOUT Y Y-COORDINATE     REM INPUT T COUNTERCLOCKWISE ROTATION IN RADIANS    X0 = X    Y0 = Y    X = X0 * COS(T) - Y0 * SIN(T)    Y = X0 * SIN(T) + Y0 * COS(T)    RETURN