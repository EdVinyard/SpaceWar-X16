INIT:    SCREEN $80    RECT 0,0, 319,239, 0    PRINT "HELLO, SPACEWAR!"        T0 = -2 * 3.14159 / 16    FOR I = 0 TO 15        X=0: Y=-4: T=I*T0: GOSUB ROTATE        PRINT X,Y        LINE 300,200, 300+X,200+Y, 1        NEXT I    ENDROTATE:    REM ROTATE AN X,Y COORDINATE AROUND THE ORIGIN (0,0)    REM INOUT X X-COORDINATE    REM INOUT Y Y-COORDINATE     REM INPUT T COUNTERCLOCKWISE ROTATION IN RADIANS    X0 = X    Y0 = Y    X = X0 * COS(T) - Y0 * SIN(T)    Y = X0 * SIN(T) + Y0 * COS(T)    RETURN