T = TI      :REM START TIMEP = RND(-TI):REM INITIALIZE PRNGP = RND(1)  :REM ACCENT COLOR DISTRIB.L = 0       :REM ADDR LOW 16 BITSH = 0       :REM ADDR HIGH BITO = 0       :REM ADDR LOW BITS OFFSETM = 65535   :REM 16 BIT INT MAXE = 320*240 :REM BITMAP SIZE BYTESF = 97      :REM MAX SPACE BETWEEN ACCENTSB = 196     :REM MIDDLE BLUE COLORSCREEN $80RECT 0,0,319,239,B :REM CLEAR SCREENDOWHILE:    R = RND(1)    C = B + INT(R*7-4)    VPOKE H, L, C    L = L + INT(F*R)    IF L > M THEN O=M:H=1:L=0    IF O+L < E GOTO DOWHILEPRINT TI-T